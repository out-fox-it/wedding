    "@graphql-codegen/add": "^3.2.1",
    "@graphql-codegen/cli": "^2.12.0",
    "@graphql-codegen/client-preset": "^1.0.2",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-resolvers": "^2.7.3",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@strv/eslint-config-node": "^3.2.0",
    "@strv/eslint-config-react": "^4.2.0",
    "@strv/eslint-config-typescript": "^3.2.0",
    "@strv/prettier-config": "^2.0.0",
    "@strv/stylelint-config-styled-components": "^2.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.6.5",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.25",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.21.0",
    "eslint-config-next": "^12.2.4",
    "eslint-plugin-import": "^2.26.0",
    "fp-ts": "^2.12.2",
    "io-ts": "^2.2.17",
    "io-ts-types": "^0.5.16",
    "jsdom": "^20.0.0",
    "monocle-ts": "^2.3.13",
    "newtype-ts": "^0.3.5",
    "node-fetch": "^2.6.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prisma": "^4.3.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^9.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
