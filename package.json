{
  "name": "wedding",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/out-fox-it/wedding"
  },
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "^16"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:css": "stylelint src",
    "lint:fix": "next lint --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "tsc": "tsc --incremental false --project .",
    "tsc:scripts": "tsc --project scripts",
    "prisma:format": "prisma format",
    "prisma:migrate": "dotenv -e .env.development -- prisma migrate dev",
    "prisma:generate": "dotenv -e .env.development -- prisma generate",
    "code:check": "run-p --print-label tsc tsc:scripts format lint lint:css prisma:format",
    "code:fix": "run-p --print-label format:fix lint:fix lint:css prisma:format",
    "graphql:generate": "graphql-codegen",
    "script": "ts-node"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@envelop/core": "^2.6.0",
    "@envelop/generic-auth": "^4.5.0",
    "@graphql-yoga/node": "^2.13.13",
    "@prisma/client": "^4.3.1",
    "bcrypt": "^5.0.1",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.18.0",
    "iron-session": "^6.2.1",
    "next": "^12.2.4",
    "next-intl": "^2.7.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "sanitize.css": "^13.0.0",
    "styled-components": "^5.3.5"
  },
